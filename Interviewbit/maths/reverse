int Solution::reverse(int A) {
    long long int n = A, tmp = 0, reverse = 0;
    while(n!=0){
        tmp = n%10;
        if(reverse *10 <= INT_MAX && reverse*10 >= INT_MIN)
            reverse = reverse*10 + tmp;
        else return 0;
        n = n/10;
    }
    if(reverse >=INT_MIN && reverse <= INT_MAX) 
        return reverse;
}
